<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>Kaka</Authors>
        <Description>A C# implementation of Mojang's Brigadier.</Description>
        <PackageProjectUrl>https://github.com/KawaiiKusa/KaLib</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/KawaiiKusa/KaLib/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/KawaiiKusa/KaLib</RepositoryUrl>
        <RepositoryType>github</RepositoryType>
        <PackageVersion>1.0.3</PackageVersion>
    </PropertyGroup>

</Project>
